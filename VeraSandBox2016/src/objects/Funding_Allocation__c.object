    <validationRules>
        <fullName>Either_Core_Training_or_Mission</fullName>
        <active>true</active>
        <description>Validation rule for either CORE checked or TRAINING filled out or MISSION filled out (can only be one)</description>
        <errorConditionFormula>OR( 
  OR( 
   NOT(Core__c), 
   ISBLANK( Mission__c ),
   ISBLANK( Training__c )
  ), 
  AND( 
   Core__c, 
   NOT(ISBLANK( Mission__c )),
   NOT(ISBLANK( Training__c )) 
  ) 
)</errorConditionFormula>
        <errorDisplayField>Core__c</errorDisplayField>
        <errorMessage>Funding must be allocated to Core or a Training or a Mission.</errorMessage>
    </validationRules>
