    <validationRules>
        <fullName>Training_or_Mission_if_not_Admin</fullName>
        <active>true</active>
        <description>Incident must be related to a Training OR a Mission if it&apos;s not an Admin incident</description>
        <errorConditionFormula>OR(
AND(
ISPICKVAL( Department__c , &apos;T&apos;),  
AND(
ISBLANK( Mission__c ),  ISBLANK(Training__c) 
)
)
,
AND(
ISPICKVAL( Department__c , &apos;M&apos;),  
AND(ISBLANK( Mission__c ),  ISBLANK(Training__c)
)
)
)</errorConditionFormula>
        <errorDisplayField>Department__c</errorDisplayField>
        <errorMessage>If Department is not Admin, please relate this Incident to either a Mission or a Training</errorMessage>
    </validationRules>
