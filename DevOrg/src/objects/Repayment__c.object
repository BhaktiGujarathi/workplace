        <formula>IF(
Intial_Repayment_Amount__c=0,&quot;No Balance&quot;,
if(  Total_Outstanding_Amount__c   /  Intial_Repayment_Amount__c  &gt; 1, 
IMAGE(&quot;/img/samples/color_red.gif&quot;, &quot;red&quot;, 15, 100),


IMAGE(&quot;/img/samples/color_green.gif&quot;, &quot;green&quot;, 15, (100 - (Total_Outstanding_Amount__c / Intial_Repayment_Amount__c) *100) ) &amp; IMAGE(&quot;/img/samples/color_yellow.gif&quot;, &quot;yellow&quot;, 15,  100 -  (100 - (Total_Outstanding_Amount__c / Intial_Repayment_Amount__c) *100)) 

))
 &amp; 
&quot; &quot; &amp; 
text(
round(
((100-(Total_Outstanding_Amount__c   /  Intial_Repayment_Amount__c)*100))
