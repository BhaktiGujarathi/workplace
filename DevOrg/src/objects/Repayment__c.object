    <validationRules>
        <fullName>Amount_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;One_Time&apos; &amp;&amp; Intial_Repayment_Amount__c!=Payment_Amount__c</errorConditionFormula>
        <errorDisplayField>Intial_Repayment_Amount__c</errorDisplayField>
        <errorMessage>Intial Repayment Amount &amp; Payment Amount must have same values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Next_Hit_Date_should_greater_than_today</fullName>
        <active>false</active>
        <errorConditionFormula>Next_Hit_Date__c   &lt;=   TODAY()</errorConditionFormula>
        <errorDisplayField>Next_Hit_Date__c</errorDisplayField>
        <errorMessage>Next Hit Date should be greater than today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Payment_Frequency_Not_Null</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;Recurring&apos; &amp;&amp; ISBLANK(TEXT(Payment_Frequency__c))</errorConditionFormula>
        <errorDisplayField>Payment_Frequency__c</errorDisplayField>
        <errorMessage>Payment Frequency can not be NULL when Record Type is Recurring.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Repaymnt_Amount_greater_than_Installment</fullName>
        <active>true</active>
        <errorConditionFormula>Intial_Repayment_Amount__c  &lt; Payment_Amount__c</errorConditionFormula>
        <errorDisplayField>Intial_Repayment_Amount__c</errorDisplayField>
        <errorMessage>Initial Payment Amount should be greater than installment Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Routing_Number_Length_Validations</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(
 OR(
  LEN (Bank_Routing_Number__c ) = 0,
  REGEX(Bank_Routing_Number__c , &quot;[0-9]{9}&quot;)
 )
)</errorConditionFormula>
        <errorDisplayField>Bank_Routing_Number__c</errorDisplayField>
        <errorMessage>Ensure that Routing Number is 9 Digits.</errorMessage>
    </validationRules>
