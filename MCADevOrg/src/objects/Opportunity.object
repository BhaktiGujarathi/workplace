        <linkType>javascript</linkType>
        <masterLabel>Deal Entry Wizard</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>//Query Submission and Offer 

{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot;)}; 
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)}

var isConsole = sforce.console.isInConsole() ? 1 : 0;
//sforce.connection.describeGlobal().sobjects; 

try 
{ 
var mcaNamePrefixValue = ((&apos;{!$Label.MCANamePrefix}&apos; == &apos;McaApp__&apos;)? &apos;{!$Label.MCANamePrefix}&apos;: &apos;&apos;);
var pathPrefix=&apos;{!$Site.Prefix}&apos;;
var result = sforce.connection.query( 
&quot;Select Name, Id from &quot;+mcaNamePrefixValue+&quot;Submission__c where &quot;+mcaNamePrefixValue+&quot;Opportunity__c =&apos;{!Opportunity.Id}&apos;&quot;); 
submission= result.getArray(&quot;records&quot;); 

var resultOffer = sforce.connection.query( 
&quot;Select Name, Id from &quot;+mcaNamePrefixValue+&quot;Offer__c where &quot;+mcaNamePrefixValue+&quot;Opportunity__c =&apos;{!Opportunity.Id}&apos;&quot;); 
offer= resultOffer .getArray(&quot;records&quot;); 

if({!ISBLANK( Opportunity.AccountId )}){ 
alert(&apos;Please associate Opportunity with Account to create Contract&apos;); 
} 
else if(submission.length &gt; 0 || offer.length &gt; 0){ 
 

window.open(pathPrefix+&quot;/apex/CreateContract?id={!Opportunity.Id}&amp;stage={!Opportunity.StageName}&amp;accId={!Opportunity.AccountId}&amp;isConsole=&quot;+isConsole,&quot;_self&quot;); 

}else{ 
alert(&quot;{!$Label.CreateContract_NoSubmissionNOfferErr}&quot;); 
} 



} 
catch(err) 
{ 

alert(&quot;You do not have sufficient privileges to access Offers or Submissions.Please contact system admin to get access.&quot;); 
}</url>
