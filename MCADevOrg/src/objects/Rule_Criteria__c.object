            </picklistValues>
            <picklistValues>
                <fullName>includes</fullName>
                <default>false</default>
                <controllingFieldValues>Multiple – Multi Select</controllingFieldValues>
            </picklistValues>
            <picklistValues>
                <fullName>excludes</fullName>
                <default>false</default>
                <controllingFieldValues>Multiple – Multi Select</controllingFieldValues>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
