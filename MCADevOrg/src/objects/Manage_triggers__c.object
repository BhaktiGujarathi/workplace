<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Will allow users to decide if they want XIRR calculations to take place on Lead or Opportunity. Name of record should be Default</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>lead_trigger_for_Xirr__c</fullName>
        <defaultValue>false</defaultValue>
        <description>XIRR will be calculated on Lead only if this is true</description>
        <externalId>false</externalId>
        <inlineHelpText>Select if XIRR should be calculated on lead</inlineHelpText>
        <label>Calculate XIRR for Lead</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>opp_trigger_for_Xirr__c</fullName>
        <defaultValue>false</defaultValue>
        <description>XIRR will be calculated on Opportunity only if this is true</description>
        <externalId>false</externalId>
        <inlineHelpText>Select if XIRR should be calculated on opportunity</inlineHelpText>
        <label>Calculate XIRR for Opportunity</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Manage Triggers</label>
    <visibility>Public</visibility>
</CustomObject>
