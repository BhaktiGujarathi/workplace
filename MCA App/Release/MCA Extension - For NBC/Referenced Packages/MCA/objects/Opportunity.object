        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}

var result = sforce.connection.query(&quot;Select Name, Id, StageName From Opportunity where Id =&apos;{!Opportunity.Id}&apos;&quot;);

records = result.getArray(&quot;records&quot;);

if(records.length &gt; 0){
window.open(&quot;/_ui/core/email/author/EmailAuthor?p3_lkid={!Opportunity.Id}&amp;retURL=%2F{!Opportunity.Id}&amp;template_id={!$Label.mcaapp__OpportunitySendApp}&amp;p2={!Opportunity.Owner_1__c}&amp;p2_lkid={!Opportunity.Owner_1Id__c}&amp;new_template=true&quot;);
try{
var opportunity = new sforce.SObject(&quot;Opportunity&quot;);
