    <webLinks>
        <fullName>Next_Lead</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Next Lead</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>if(localStorage[&apos;CycleLeads&apos;]){
    var CycleLeads = localStorage[&apos;CycleLeads&apos;].split(&apos;,&apos;);
    var currentLeadPos = CycleLeads.indexOf(&apos;{!Lead.Id}&apos;);

    if(currentLeadPos == CycleLeads.length - 1){
        alert(&apos;This was the Last Lead!&apos;);
    }
    else{
        location.replace(&apos;/&apos; + CycleLeads[currentLeadPos + 1]);
    }
}</url>
    </webLinks>
