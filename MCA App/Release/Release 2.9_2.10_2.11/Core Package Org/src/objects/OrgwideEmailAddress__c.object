<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>The custom setting is used to allow use of org wide email for outgoing email sent from  submission wizard</description>
    <fields>
        <fullName>IsOrgwideEmailAllowed__c</fullName>
        <defaultValue>false</defaultValue>
        <description>The check will determine whether to use org wide email or not for the email sent out from submission wizard</description>
        <label>IsOrgwideEmailAllowed</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>OrgEmailAddress__c</fullName>
        <label>OrgEmailAddress</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <label>OrgwideEmailAddress</label>
</CustomObject>
