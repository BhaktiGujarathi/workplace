        <fullName>Contract_Requested_This_Month__c</fullName>
        <externalId>false</externalId>
        <formula>and(MONTH( Contract_Requested_Date__c ) = MONTH(TODAY()) , YEAR( Contract_Requested_Date__c ) = YEAR(TODAY()))</formula>
