    <fields>
        <fullName>Contract_Requested_This_Week__c</fullName>
        <externalId>false</externalId>
        <formula>MOD(TODAY() - DATE(1900, 1, 7), 7) &gt;= MOD(  Contract_Requested_Date__c  - DATE(1900, 1, 7), 7) &amp;&amp; 
