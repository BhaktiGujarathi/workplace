<apex:page StandardController="Case" extensions="CaseHighlightPanelController" action="{!findCase}">
<style>
            .sfdc_richtext img {
               border: 0;
               width: 70px;
               height: 70px;
             }
        </style>

<apex:pageBlock >

        <apex:pageBlockTable value="{!lstCase}" var="c">            
           <apex:column headerValue="Photo">
            <apex:outputField value="{!c.Contact.Image_del__c}"/>
           </apex:column>
             <apex:column value="{!c.Account.Name}"/>
             
             <apex:column value="{!c.Contact.Email}"/>

        </apex:pageBlockTable>    
 

    </apex:pageBlock> 
</apex:page>