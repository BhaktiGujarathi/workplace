                    <script type="text/javascript" >
                        function noenter(e){
                            if(window.event){
                                key = window.event.keyCode; //IE
                            }else{
                                key = e.which; //firefox
                            }
                            if(key == 13) {
                                var ele=document.getElementById("{!$Component.filterBtn}").id;
                                document.getElementById(ele).click();
                                return false;
                            } else{
                                return true;
                            }
