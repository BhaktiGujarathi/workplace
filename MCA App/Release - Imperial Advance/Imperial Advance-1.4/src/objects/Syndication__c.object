<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <enableActivities>true</enableActivities>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Anticipated_Term_Months__c</fullName>
        <formula>Opportunity__r.Term_Months__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Anticipated Term - Months</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>Commission_Fee__c</fullName>
        <inlineHelpText>Amount paid towards commissions</inlineHelpText>
        <label>Commission Fee $</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Deal_Funding_Amount__c</fullName>
        <formula>Opportunity__r.Funding_Amount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Deal Funding Amt</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Deal_Payback_Amount__c</fullName>
        <formula>Opportunity__r.Payback_Amount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Deal Payback Amt</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Disbursment_Schedule__c</fullName>
        <label>Disbursement Schedule</label>
        <picklist>
            <picklistValues>
                <fullName>Daily</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Weekly</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>bi-weekly</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Monthly</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Interest Only</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Estimated_Repayment_Date__c</fullName>
        <formula>Opportunity__r.Date_Funded__c + (Opportunity__r.Term_Months__c *30.4368)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Esti. Repay Date</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Memo__c</fullName>
        <label>Memo</label>
        <length>32768</length>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Opportunity__c</fullName>
        <label>Opportunity</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>Syndication</relationshipLabel>
        <relationshipName>Syndication</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <type>MasterDetail</type>
    </fields>
    <fields>
        <fullName>Profit_Amount__c</fullName>
        <formula>Repayment_Amt__c -  Syndication_Amt__c -  Servicing_Fee__c -  Commission_Fee__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Profit $</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Profit_Percentage__c</fullName>
        <formula>IF(Syndication_Amt__c&gt;0,Profit_Amount__c /  Syndication_Amt__c,null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Profit %</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Projected_Monthly_Pmt_Amt__c</fullName>
        <formula>IF(Opportunity__r.Term_Months__c&gt;0, Repayment_Amt__c / Opportunity__r.Term_Months__c ,0)</formula>
        <label>Projected Monthly Pmt Amt</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>ROR_Dollar__c</fullName>
        <formula>Syndication_Amount__c *  ROR_Percentage__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ROR$</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>ROR_Percentage__c</fullName>
        <label>ROR%</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>ROR_per__c</fullName>
        <formula>IF(Syndication_Amt__c &gt; 0 , 
((Syndication_Amt__c  *   Opportunity__r.Factor_Rate__c  ) -  Servicing_Fee__c - Commission_Fee__c  ) /  Syndication_Amt__c,null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ROR %</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Repayment_Amt__c</fullName>
        <formula>( Deal_Payback_Amount__c *  Syndication_per__c  ) -   Servicing_Fee__c -  Commission_Fee__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Repayment Amt</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Servicing_Bank__c</fullName>
        <formula>Opportunity__r.Bank_Name__r.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Servicing Bank</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Servicing_Fee__c</fullName>
        <inlineHelpText>Management Fee charged by the Bank that is servicing the deal</inlineHelpText>
        <label>Servicing Fee</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Syndicated_By__c</fullName>
        <inlineHelpText>The entity that is participating on the deal</inlineHelpText>
        <label>Syndicated By</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Syndication</relationshipLabel>
        <relationshipName>Syndication</relationshipName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Syndication_Amount__c</fullName>
        <formula>Opportunity__r.Funding_Amount__c *  Syndication_Percentage__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Syndication Amount</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Syndication_Amt__c</fullName>
        <inlineHelpText>Participation amount</inlineHelpText>
        <label>Syndication Amt $</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Syndication_Fee__c</fullName>
        <label>Syndication Fee</label>
        <precision>12</precision>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Syndication_Percentage__c</fullName>
        <label>Syndication %</label>
        <precision>12</precision>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Syndication_per__c</fullName>
        <formula>IF( Deal_Funding_Amount__c  &lt;&gt; 0, Syndication_Amt__c  /  Deal_Funding_Amount__c , Syndication_Amt__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Syndication %</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <label>Syndication</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Deal_Funding_Amount__c</columns>
        <columns>Servicing_Bank__c</columns>
        <columns>Opportunity__c</columns>
        <columns>Syndication_Percentage__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <displayFormat>SYN-{00000}</displayFormat>
        <label>Syndication Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Syndications</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
</CustomObject>
