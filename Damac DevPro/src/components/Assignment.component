<script>
    $(document).ready(function(){
        validateStep1Completeness();
    });
    
    $('.well input.datepicker').datepicker({
    });
    
    function validateStep1Completeness(){
        var selectedUnit = $('select[id$=units]').val();
        var selectedBuyerType = $('.buyerType').val();
        var selectedRelationship = $('.relationship').val();
        var selectedCaseType = $('.caseType').val();
        
        if(selectedUnit != undefined && selectedUnit != ''
        && selectedBuyerType != undefined && selectedBuyerType != ''
        && selectedRelationship != undefined && selectedRelationship != ''
        && selectedCaseType != undefined && selectedCaseType != ''){
            $('.viewRequirement').removeAttr('disabled');
        }else{
            $('.viewRequirement').attr('disabled','disabled');
        }
    }
    
    function validateDocumentCompleteness(){
        $('.inputFile').each(function(){
            if($(this).val().trim() != undefined && $(this).val().trim() != ''){
                $(this).addClass('success'));
                $('.docsNextButton').removeAttr('disabled');
            }else{
                $('.docsNextButton').attr('disabled','disabled');
                return false;
            }
        });
    }
</script>
