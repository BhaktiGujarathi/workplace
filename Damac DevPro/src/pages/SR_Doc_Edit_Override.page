<apex:page id="myPage" standardController="NSIBPM__SR_Doc__c" extensions="SR_Docs_Edit_Delete_Override_Controller" sidebar="false" showHeader="false">
    <script>
    
    if ( {!canEditRecord} ) {
        window.parent.location.href = "{!URLFOR($Action.NSIBPM__SR_Doc__c.Edit,NSIBPM__SR_Doc__c.id,[retURL=URLFOR($Action.NSIBPM__SR_Doc__c.View,NSIBPM__SR_Doc__c.id)],true)}";
    } else {
        alert("You are not allowed to edit the SR Doc.");
        window.history.go(-1);
    }
    </script>
</apex:page>