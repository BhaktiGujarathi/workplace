<apex:page controller="DamacCompanyProfileController" action="{!init}">
    <html>
        <head>
            <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
            <meta content="utf-8" http-equiv="encoding"/>
            <meta name="viewport" content="width=device-width, initial-scale=1"/>
            <title>Agent Portal- Company Profile</title>
            <apex:stylesheet value="{!URLFOR($Resource.AgentPortal_Resources, 'assets/css/damac-theme.css')}" />
            <apex:stylesheet value="{!URLFOR($Resource.AgentPortal_Resources, 'assets/css/dataTable.css')}" />
            <style>
                
                .subcontainer{
                padding:10px;
                }
                
                .detailPage-section-heading{
                padding:10px;
                border-bottom:2px solid #0D80FC !important;
                color: black !important;
                font-weight: bold !important;
                }
                
                .detailPage-label{
                font-size: 12px !important;
                font-family: Arial,sans-serif;
                color:#A8A8A8 !important;
                text-align:right !important;
                }
                
                .detailPage-text{
                font-size: 12px !important;
                font-family: Arial,sans-serif;
                color:black !important;
                text-align:left !important;
                }
                
                .detail-page-content-border{
                border:1px solid  #F6F6F6;
                }
                
                .border-bottom{
                border-bottom:1px solid #F6F6F6;
                }
                
                .custom-table thead > tr > th{
                width:100px !important;
                }
                
                .dataTables_filter{
                width: 100% !important;
                }
                
                /*Change specific to this page*/
                @media only screen and (max-width: 767px){
                .height-100vh {
                width: 93vw !important;
                margin:0px !important;
                margin-top: 10px !important; 
                }
                
                .role{
                display:none !important;
                }
                }
                
                
                .header-home{
                background:url('{!URLFOR($Resource.AgentPortal_Resources, 'assets/images/Company_Profile.jpg')}') no-repeat !important;
                -webkit-background-size: cover!important;
                -moz-background-size: cover!important;
                background-size: cover!important;
                position:relative;
                }
            </style>
        </head>
        <body >
            <div class="global-wrapper content-bg">
                <apex:insert name="body">
                    
                    <section class="global-content-w-footer slds" id="company-profile-section">
                        <div class="subcontainer">
                            
                            <div class="slds-grid slds-wrap">
                                <apex:outputPanel rendered="{!$Permission.IsAdmin}" layout="Block" style="width:100%;text-align:center">
                                    <apex:outputPanel rendered="{!isSRPending == false}" styleClass="slds-container--large slds-container--center slds-p-around--large">
                                        <a href="apex/Process_Flow?FlowId={!registrationUpdatePageID}" class="slds-button slds-button--neutral">Edit</a>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!isSRPending == true}" styleClass="slds-container--large slds-container--center slds-p-around--large">
                                        <a href="apex/Process_Flow?FlowId={!registrationUpdatePageID}&Id={!serviceRequestID}" class="slds-button slds-button--neutral">Pending SR</a>
                                    </apex:outputPanel>
                                </apex:outputPanel>
                            </div>
                            
                            <apex:outputPanel id="individual" rendered="{!accountDetail.ispersonaccount}">
                                <!-- Basic Information Section Individual-->
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--12-of-12 section-header text-medium detailPage-section-heading">
                                        Basic Information
                                    </div>
                                </div>
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        First Name   
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.FirstName}
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Last Name
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.LastName} 
                                    </div>
                                </div>
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                                
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        ID Type
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.ID_Type__pc}
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Broker Card Expiry Date
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.Broker_Card_Expiry_Date__pc} 
                                    </div>
                                </div>
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                                
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        ID Number
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.ID_Number__pc}
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Broker Card Expiry Date
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.Broker_Card_Number__pc} 
                                    </div>
                                </div>
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                                
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        ID Expiry Date
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.ID_Expiry_Date__pc}
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Designation
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.Designation__pc} 
                                    </div>
                                </div>
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                                
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Nationality
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.Nationality__pc}
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Eligible to Sell in Dubai
