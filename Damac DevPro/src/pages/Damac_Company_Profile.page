                                        {!accountDetail.Trade_License_Number__c}   
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Trade License Expiry Date
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        <apex:outputText value="{0,date,dd/MM/yyyy}"> 
                                            <apex:param value="{!accountDetail.Trade_License_Expiry_Date__c}" /> 
                                        </apex:outputText>  
                                    </div>
                                </div>
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Date Of Incorporation
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        <apex:outputText value="{0,date,dd/MM/yyyy}"> 
                                            <apex:param value="{!accountDetail.Date_of_Incorporation__c}" /> 
                                        </apex:outputText>   
                                    </div>
                                    
                                    
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        Country Of Incorporation
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.Country_of_Incorporation_New__c} 
                                    </div>
                                </div>
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        ORN Number
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.ORN_Number__c} 
                                    </div>
                                    
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        City Of Incorporation
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        {!accountDetail.City_of_Incorporation_New__c} 
                                    </div>
                                </div>
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        RERA Expiry Date
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        <apex:outputText value="{0,date,dd/MM/yyyy}">
                                            <apex:param value="{!accountDetail.RERA_Expiry_Date__c}"></apex:param>
                                        </apex:outputText>
                                    </div>
                                    
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
                                        
                                    </div>
                                    <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-text">
                                        
                                    </div>
                                </div>  
                                <div class="slds-size--12-of-12 detail-page-content-border"></div>
                            </apex:outputPanel>
                            <div class="slds-grid slds-wrap">
                                <div class="slds-size--12-of-12 section-header text-medium detailPage-section-heading">
                                    Emails
                                </div>
                            </div>     
                            
                            
                            <div class="slds-grid slds-wrap">
                                <div class="slds-size--6-of-12 slds-large-size--3-of-12 slds-p-around--small detailPage-label">
