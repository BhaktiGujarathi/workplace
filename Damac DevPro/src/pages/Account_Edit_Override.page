<apex:page id="myPage" standardController="Account" extensions="Account_Edit_Delete_Override_Controller" sidebar="false" showHeader="false">
    <script>
    if ( {!canEditRecord} ) {
        window.parent.location.href = "{!URLFOR($Action.Account.Edit,Account.id,[retURL=URLFOR($Action.Account.View,Account.id)],true)}";
    } else {
        alert("You are not allowed to edit the Agency.");
        window.history.go(-1);
    }
    </script>
</apex:page>