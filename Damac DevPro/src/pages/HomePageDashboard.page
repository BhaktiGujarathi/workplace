<apex:page controller="DashboardExample" sidebar="false" showheader="false" action="{!submit}"
    docType="html-5.0" title="Bar chart">
    <style>
    html{
    background-color:#f9f9f9 !important;
    }
    </style>
    <div style="width:100%">
    <div style="width:33%;float:left;">
    <apex:chart id="chartid" height="100" width="100%" data="{!data}"
        colorSet="#42f4b3" resizable="true">
        <apex:axis type="Numeric" position="left" fields="count"
            title="" minimum="0" grid="false" />
        <apex:axis type="Category" position="bottom" fields="billingcityname"
            title="" />
        <apex:barSeries orientation="vertical" axis="left"
            xField="billingcityname" yField="count">
        </apex:barSeries>
    </apex:chart>
    </div>
    <div style="width:33%;float:left;">
        <apex:chart id="chartid1" height="100" width="100%" data="{!data}"
        colorSet="orange" resizable="true">
        <apex:axis type="Numeric" position="left" fields="count"
            title="" minimum="0" grid="false" />
        <apex:axis type="Category" position="bottom" fields="billingcityname"
            title="" />
        <apex:barSeries orientation="vertical" axis="left"
            xField="billingcityname" yField="count">
        </apex:barSeries>
    </apex:chart>
</div>
</div>
<div style="width:33%;float:left;">
    <apex:chart id="chartid2" height="100" width="100%" data="{!data}"
        colorSet="green" resizable="true">
        <apex:axis type="Numeric" position="left" fields="count"
            title="" minimum="0" grid="false" />
        <apex:axis type="Category" position="bottom" fields="billingcityname"
            title="" />
        <apex:barSeries orientation="vertical" axis="left"
            xField="billingcityname" yField="count">
        </apex:barSeries>
    </apex:chart>
</div>

</apex:page>