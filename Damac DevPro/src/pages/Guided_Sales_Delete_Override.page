<apex:page id="myPage" standardController="Guided_Sales__c" extensions="GuidedSalesEditDeleteOverrideCont" sidebar="false" showHeader="false">
    <script>
    
    if ( {!canDeleteRecord} ) {
        window.parent.location.href = "{!URLFOR($Action.Guided_Sales__c.Delete,Guided_Sales__c.id,[retURL=$Request.retURL],true)}";
    } else {
        alert("You are not allowed to delete the Guided Sales.");
        window.history.go(-1);
    }
    </script>
</apex:page>