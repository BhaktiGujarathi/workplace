        </head>
        <body style="background-color:#F7F9FB;">
            <div class="slds divbackgroundColor">
                <!--Spinner Start-->
                <apex:actionStatus id="status">
                    <apex:facet name="start">
                        <div>
                            <div class="slds-spinner_container" style="position:fixed">
                                <div  class="slds-spinner slds-spinner--medium slds-spinner--brand" role="alert">
                                    <span class="slds-assistive-text">Loading</span>
                                    <div class="slds-spinner__dot-a"></div>
                                    <div class="slds-spinner__dot-b"></div>
                                </div>
                            </div>
                        </div>
                    </apex:facet>
                </apex:actionStatus>
                <!--Spinner End-->
                <!--HEADER-->
                <div class="slds-container slds-p-left--small slds-p-right--small" role="banner" style="margin-top:0.75rem;">
                    <h1 class="slds-page-header__title slds-text-color--default slds-p-around--small" title="Guided Sales Process" style="background-color:#0070d2;">Guided Sales Process</h1>
                </div>
                <!--HEADER Complete-->
                <!-- Error Block-->
                <div class="slds-container slds-p-around--small">
                    <!--Error Block Start-->
                    <apex:outputPanel layout="block" id="errorBlock">
                        <apex:outputPanel layout="block" styleClass="slds-notify_container" rendered="{!NOT(ISBLANK(errorMessage))}">
                            <div class="slds-notify slds-notify--toast {!IF(isError,'slds-theme--error',IF(isWarning,'slds-theme--warning',IF(isSuccess,'slds-theme--success','')))} " role="alert" id="errorBox"
                                xmlns="http://www.w3.org/2000/svg"
                                xmlns:xlink="http://www.w3.org/1999/xlink">
                                <span class="slds-assistive-text">Error</span>
                                <button class="slds-button slds-notify__close slds-button--icon-inverse" title="Close" onclick="j$('[id$=errorBox]').hide();">
                                    <svg class="slds-button__icon slds-button__icon--large" aria-hidden="true">
                                        <use id="errorClose" xlink:href="{!URLFOR($Resource.SLDS_VF214, '/assets/icons/utility-sprite/svg/symbols.svg#close')}"></use>
                                    </svg>
                                    <span class="slds-assistive-text">Close</span>
                                </button>
                                <div class="slds-notify__content slds-grid">
                                    <svg class="slds-icon slds-icon--small slds-m-right--small slds-col slds-no-flex" aria-hidden="true">
                                        <use xlink:href="{!URLFOR($Resource.SLDS_VF214, '/assets/icons/utility-sprite/svg/symbols.svg#warning')}"></use>
                                    </svg>
                                    <div class="slds-col slds-align-middle">
                                        <label class="slds-text-heading--small" style="display: {!IF(isError,'block','none')}" id="errorPrefix">Oops, You&#x27;ve encountered some errors.</label>
                                        <apex:outputText style="color: #FFFFFF" value="{!errorMessage}" escape="false"/>
                                    </div>
                                </div>
                            </div>
                        </apex:outputPanel>
                    </apex:outputPanel>
                    <!--Error Block End-->
                    <div class="slds-theme--shade">
                        <apex:form id="GSPForm">
                            <!--html-oninput="sizestart.value = document.getElementById('gspPage:GSPForm:sizestartsqft').value;sizeend.value=document.getElementById('gspPage:GSPForm:sizeendsqft').value;-->
                            <!--Action Functions Section-->
                            <apex:actionFunction name="goToNextPage" status="status" action="{!navigatePages}" reRender="GSPForm,errorBlock,scriptTag" oncomplete="invokePagination({!pagesToShowList.size});">
                                <apex:param name="currentPage" assignTo="{!currentPage}" value=""/>
                                <apex:param name="nextPage" assignTo="{!nextPage}" value="" />
                                <apex:param name="purposefor" assignTo="{!purposefor}" value="j$('input[name=options]:checked').parent().find('.slds-form-element__label').text()"/>
                                <apex:param name="selectedProjectID" assignTo="{!selectedProjectID}" value=""/>
                                <apex:param name="selectedProjectName" assignTo="{!selectedProjectName}" value=""/>
                                <apex:param name="selectedInventory" assignTo="{!selectedInventory}" value=""/>
                                <apex:param name="backFromInventory" assignTo="{!backFromInventory}" value=""/>
                            </apex:actionFunction>
                            <apex:actionFunction name="inventoryDetail" action="{!inventoryDetailPage}" reRender="inventoryDetails,errorBlock,scriptTag">
                                <apex:param name="selectedInventory"  assignTo="{!selectedInventory}" value=""/>
                            </apex:actionFunction>
                            <!-- <apex:actionFunction name="getPreviousProperties" action="{!getPreviousPropertiesRecords}" status="status" reRender="gspallProperties,errorBlock,scriptTag"/><apex:actionFunction name="getnextProperties" action="{!getnextPropertiesRecords}" status="status" reRender="gspallProperties,errorBlock,scriptTag"/> -->
                            <apex:actionfunction name="Invoke_Goto_Page" action="{!showRecords}" rerender="relatedInventories,scriptTag" onComplete="invokePagination('{!pagesToShowList.size}');" status="status">
                                <apex:param name="pgno" value="" assignto="{!pageNumber}" />
                            </apex:actionfunction>
                            <apex:actionFunction name="getWalkthroughUrl" action="{!getWalkthroughUrl}" status="status" oncomplete="redirect('{!redirectUrl}');">
                                <apex:param name="url" value="" assignTo="{!baseUrl}" />
                            </apex:actionFunction>
                            <!--Action Functions Section-->
                            <!--Home Page Start-->
                            <apex:outputPanel style="width:100%" styleClass="homePage slds-theme--shade slds-grid slds-wrap slds-wrap-override" layout="block" id="gspHome" rendered="{!displayHomePage}">
                                <div class="slds-size--1-of-5 slds-small-size--1-of-7 slds-medium-size--1-of-9 slds-large-size--1-of-12 slds-box slds-text-align--center">
                                    <!--slds-grid slds-wrap slds-grid--vertical -->
                                    <label class="titleColor slds-m-top--small">Guided Sections</label>
                                    <hr class="slds-hr slds-m-top--none slds-m-bottom--none"/>
                                    <a href="javascript:void(0);" id="section1" onclick="selectHomeSection(1);" class="slds-navigation-list--vertical__action slds-text-link--reset slds-m-top--small slds-m-bottom--small" >
                                        <!-- <div class=""> -->
                                            <span id="whyDamacHeader" class="show">Why Damac</span>
                                            <img id="whyDamacHeader_active_icon" class="activeDamac  show" style="display: inline;" src="{!URLFOR($Resource.GSP_Icons, 'GSP_Icons/selected/Why_Damac.png')}" />
                                            <img id="whyDamacHeader_inactive_icon" class="inactiveDamac hide" style=""  src="{!URLFOR($Resource.GSP_Icons, 'GSP_Icons/notselected/Why_Damac.png')}" />
                                        <!-- </div> -->
                                    </a>
                                    <a href="javascript:void(0);" id="section2" onclick="selectHomeSection(2);" class="slds-navigation-list--vertical__action slds-text-link--reset slds-m-top--small slds-m-bottom--small" >
                                        <!-- <div class=""> -->
                                            <span id="whyDubaiHeader" class="show">Why Dubai</span>
                                            <img id="whyDubaiHeader_inactive_icon" class="inactiveDubai show" style="display: inline;" src="{!URLFOR($Resource.GSP_Icons, 'GSP_Icons/notselected/Why_Dubai.png')}"></img>
                                            <img id="whyDubaiHeader_active_icon" class=" activeDubai hide" style="" src="{!URLFOR($Resource.GSP_Icons, 'GSP_Icons/selected/Why_Dubai.png')}"></img>
                                        <!-- </div> -->
                                    </a>
                                </div>
                                <div class="slds-box slds-size--4-of-5 slds-small-size--6-of-7 slds-medium-size--8-of-9 slds-large-size--11-of-12" style="border-left: 10px solid #f4f6f9">
                                    <div class="activeDamac slds-p-left--small show slds-p-right--small" >
                                        <label id="damactitle" class="titleColor   slds-m-top--small inactiveDubai show" style="display:inline;">Why Damac</label>
                                        <hr class="slds-hr slds-m-bottom--small inactiveDubai slds-m-top--none show"/>
                                        <div id="damacdetailContainer" class="aspect-ratio" style="overflow-y: scroll; ">
                                            <img id="damacdetail" class="inactiveDubai show" src="{!$Resource.whydamac}" />
                                        </div>
                                    </div>
                                    <div class="hide activeDubai slds-p-left--small slds-p-right--small" >
                                        <label id="dubaititle" class="slds-m-top--small titleColor activeDubai">Why Dubai</label>
                                        <hr class="slds-hr slds-m-bottom--small slds-m-top--none activeDubai"/>
                                        <!-- slds-form-element__label slds-p-bottom--large titleColor activeDubai-->
                                        <div id="dubaidetailContainer" class="aspect-ratio" style="overflow-y: scroll;">
                                            <img id="dubaidetail" class="activeDubai hide" src="{!$Resource.WhyDubai}" />
                                        </div>
                                    </div>
                                    <!--Buttons-->
                                    <div class="slds-m-top--small slds-m-bottom--small slds-align--absolute-center">
                                        <div class="slds-p-right--small">
                                            <apex:commandButton value="Cancel" immediate="true"  action="{!cancel}" styleClass="slds-button slds-button--neutral slds-button--neutral-cancel" rerender="GSPForm,errorBlock,scriptTag" status="status"/>
                                        </div>
                                        <div>
                                            <apex:commandButton value="Next" onclick="goToNextPage(1,2,'','','','',false); return false;" styleClass="slds-button slds-button--brand slds-button--brand-next"/>
                                        </div>
                                    </div>
                                    <!--Buttons-->
                                </div>
                            </apex:outputPanel>
                            <!--Home Page End-->
                            <!--Enquiry Page Start -->
                            <apex:outputPanel id="gspInquiry" style="width:100%;" styleClass=" inquiryPage slds-theme--shade slds-form-element" layout="block" rendered="{!displayInquiryPage}">
                                <div class="slds-size--1-of-1 aspect-ratio" style="overflow-y:scroll;">
                                    <div class="slds-row slds-box slds-p-left--small slds-p-right--small">
                                        <div class="slds-form-element slds-p-left--small slds-p-right--small">
                                            <abbr class="slds-required slds-m-left--none slds-m-right--none" title="required">*</abbr>
                                            <label class="slds-form-element__legend slds-form-element__label slds-p-right--xx-large slds-p-bottom--x-small">Expected Usage of the Property</label>
                                            <div class="slds-form-element-control slds-p-left--small slds-p-right--small">
                                                <span class="slds-checkbox">
                                                    <label class="slds-checkbox__label" >
                                                        <apex:inputCheckbox id="Regular" styleClass="sldsCheckbox" value="{!regularReturn}"/>
                                                        <span class="slds-checkbox--faux"></span>
                                                        <span class="slds-form-element__label">Regular Return on Investment</span>
                                                    </label>
                                                </span>
                                                <span class="slds-checkbox">
                                                    <label class="slds-checkbox__label" >
                                                        <apex:inputCheckbox id="Buy" styleClass="sldsCheckbox" value="{!buyandHold}"/>
                                                        <span class="slds-checkbox--faux"></span>
                                                        <span class="slds-form-element__label">Buy and Hold for Investment Growth</span>
                                                    </label>
                                                </span>
                                                <span class="slds-checkbox">
                                                    <label class="slds-checkbox__label" >
                                                        <apex:inputCheckbox id="Retirement" styleClass="sldsCheckbox" value="{!retirement}"/>
                                                        <span class="slds-checkbox--faux"></span>
                                                        <span class="slds-form-element__label">Retirement Use</span>
                                                    </label>
                                                </span>
                                                <span class="slds-checkbox">
                                                    <label class="slds-checkbox__label" >
                                                        <apex:inputCheckbox id="personel" styleClass="sldsCheckbox" value="{!personalUse}"/>
                                                        <span class="slds-checkbox--faux"></span>
                                                        <span class="slds-form-element__label">Personal Use</span>
                                                    </label>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <!--Location Section Start-->
                                    <div class="slds-row slds-box slds-m-top--small slds-form--inline slds-p-left--small slds-p-right--small">
                                        <div class="slds-p-bottom--x-small slds-p-left--small slds-p-right--small">
                                            <label class="slds-form-element__legend titleColor">Location</label>
                                            <hr class="slds-hr slds-m-top--none slds-m-bottom--small"/>
                                        </div>
                                        <div class="slds-form-element slds-p-left--small slds-p-right--small slds-size--1-of-1 slds-medium-size--1-of-4">
                                            <abbr class="slds-required slds-m-left--none slds-m-right--none" title="required">*</abbr>
                                            <label class="slds-form-element__label" for="city">City</label>
                                            <div class="slds-form-element-control">
                                                <apex:selectList styleClass="slds-input" size="1" value="{!newGuidedSales.City__c}">
                                                    <apex:selectOptions value="{!allCities}"/>
                                                    <!-- <apex:actionSupport event="onchange" action="{!getAllInventoryDistrict}" status="status" reRender="gspInquiry,errorBlock" oncomplete="invokePagination({!pagesToShowList.size});"/> -->
                                                </apex:selectList>
                                                <!--<apex:inputField id="city" styleClass="slds-input" value="{!newGuidedSales.City__c}"></apex:inputField>-->
                                            </div>
                                        </div>
                                    </div>
                                    <!--Location Section End-->
                                    <!--Project Section Start-->
                                    <div class="slds-row slds-box slds-m-top--small slds-form--inline slds-p-left--small slds-p-right--small">
                                        <div class="slds-p-bottom--x-small slds-p-left--small slds-p-right--small">
                                            <label class="slds-form-element__legend titleColor">Project</label>
                                            <hr class="slds-hr slds-m-top--none slds-m-bottom--small"/>
                                        </div>
                                        <div class="slds-form-element slds-p-left--small slds-p-right--small slds-size--1-of-1 slds-medium-size--1-of-4">
                                            <abbr class="slds-required slds-m-left--none slds-m-right--none" title="required">*</abbr>
                                            <label class="slds-form-element__label" for="district">Master Development</label>
                                            <div class="slds-form-element-control">
                                                <apex:selectList styleClass="slds-input" id="district" size="1"  value="{!newGuidedSales.Master_Development__c}"> <!-- disabled="{!ISBLANK(newGuidedSales.City__c)}" -->
                                                    <apex:selectOptions value="{!allPropertyDistricts}"/>
                                                    <!-- <apex:actionSupport event="onchange" action="{!getAllInventoryProjects}" reRender="gspInquiry,errorBlock" status="status" oncomplete="invokePagination({!pagesToShowList.size});"/> -->
                                                </apex:selectList>
                                            </div>
                                        </div>
                                        <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-4">
                                            <abbr class="slds-required slds-m-left--none slds-m-right--none" title="required">*</abbr>
                                            <label class="slds-form-element__label" for="project">Project</label>
                                            <div class="slds-form-element-control">
                                                <apex:selectList styleClass="slds-input" id="project" size="1"  value="{!newGuidedSales.Project__c}"> <!-- disabled="{!ISBLANK(newGuidedSales.Master_Development__c)}" -->
                                                    <apex:selectOptions value="{!allProjects}"/>
                                                    <!-- <apex:actionSupport event="onchange" action="{!getAllPicklistValues}" reRender="gspInquiry,errorBlock" status="status" oncomplete="invokePagination({!pagesToShowList.size});"/> -->
                                                </apex:selectList>
                                                <!--<apex:inputField id="project" styleClass="slds-input" value="{!newGuidedSales.Project__c}"></apex:inputField>-->
                                            </div>
                                        </div>
                                        <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-4">
                                            <label class="slds-form-element__label" for="projectC">Project Category</label>
                                            <div class="slds-form-element-control">
                                                <apex:selectList styleClass="slds-input" size="1" id="projectC"  value="{!newGuidedSales.Project_Category__c}"> <!-- disabled="{!ISBLANK(newGuidedSales.Project__c)}" -->
                                                    <apex:selectOptions value="{!allProjectsCategory}"/>
                                                </apex:selectList>
                                                <!--<apex:inputField id="projectcategory" styleClass="slds-input" value="{!newGuidedSales.Project_Category__c}"></apex:inputField>-->
                                            </div>
                                        </div>
                                        <div class="slds-has-divider--bottom slds-size--1-of-1 slds-m-top--x-small slds-m-bottom--x-small"/>
                                        <div class="slds-form-element slds-p-left--small slds-p-right--small slds-size--1-of-1 slds-medium-size--1-of-4">
                                            <abbr class="slds-required slds-m-left--none slds-m-right--none" title="required">*</abbr>
                                            <label class="slds-form-element__label" for="type">Type</label>
                                            <div class="slds-form-element-control">
                                                <apex:selectList styleClass="slds-input" size="1" id="type" value="{!newGuidedSales.Type__c}"> <!-- disabled="{!ISBLANK(newGuidedSales.Project__c)}" -->
                                                    <apex:selectOptions value="{!allUnitTypes}"/>
                                                </apex:selectList>
                                                <!--<apex:inputField id="type" styleClass="slds-input" value="{!newGuidedSales.Type__c}"></apex:inputField>-->
                                            </div>
                                        </div>
                                        <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-4">
                                            <abbr class="slds-required slds-m-left--none slds-m-right--none" title="required">*</abbr>
                                            <label class="slds-form-element__label" for="bType">Bedroom Type</label>
                                            <div class="slds-form-element-control">
                                                <apex:selectList styleClass="slds-input" size="1" id="bType"  value="{!newGuidedSales.Bedroom_Type__c}"> <!-- disabled="{!ISBLANK(newGuidedSales.Project__c)}" -->
                                                    <apex:selectOptions value="{!allBedroomTypes}"/>
                                                </apex:selectList>
                                                <!--<apex:inputField id="bedroomtype" styleClass="slds-input" value="{!newGuidedSales.Bedroom_Type__c}"></apex:inputField>-->
                                            </div>
                                        </div>
                                        <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-4">
                                            <label class="slds-form-element__label" for="project1">Type of Booking</label>
                                            <div class="slds-form-element-control">
                                                <apex:inputField id="typeofbooking" styleClass="slds-input" value="{!newGuidedSales.Type_of_Booking__c}"></apex:inputField>
                                            </div>
                                        </div>
                                        <div class="slds-has-divider--bottom slds-size--1-of-1 slds-m-top--x-small slds-m-bottom--x-small"/>
                                        
                                        <div class="slds-form-element slds-size--1-of-1 slds-medium-size--1-of-4 slds-p-left--small slds-p-right--small">
