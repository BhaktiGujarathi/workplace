<apex:page id="myPage" standardController="Booking_Unit__c" extensions="Booking_Unit_Edit_Delete_Override" sidebar="false" showHeader="false">
    <script>
    
    if ( {!canEditRecord} ) {
        window.parent.location.href = "{!URLFOR($Action.Booking_Unit__c.Edit,Booking_Unit__c.id,[retURL=URLFOR($Action.Booking_Unit__c.View,Booking_Unit__c.id)],true)}";
    } else {
        alert("You are not allowed to edit the Booking Unit.");
        window.history.go(-1);
    }
    </script>
</apex:page>