<script>
    $(document).ready(function() {
        $(".nav-tabs a").click(function() {
            $(this).tab('show');
        });
        $(".datepicker").datepicker({
            format: 'dd/mm/yyyy'
        });

        // Nicescroll
        $(".notification-panel .panel-body").niceScroll({
            cursorcolor: "#6F7E95",
            cursorborder: "1px solid #6F7E95"
        });

        // For Slider Button control
        $('.arrow-left').click(function() {
            $('.carousel').carousel('prev');
        });
        $('.arrow-right').click(function() {
            $('.carousel').carousel('next');
        });

        $(".circular-progress-bar").loading();



        /*** Close dropdown - accordian when another opens ***/
        $('.pro-planning-link').on("click", function(event) {
            $('.panel-collapse').each(function() {
                if ($(this).hasClass('in')) {
                    $(this).collapse('toggle');
                }
            });
        });



        // sidebar toggle on mobile
        $('.bodyOverlay').hide();

        flag = 0;
        $(".menu-button").click(function() {
            $('.sidebar-wrapper').css('transition', 'all 0.5s ease');
            if (flag == 0) {
                $('.sidebar-wrapper').show();
                $('.sidebar-wrapper').css('margin-left', '0');
                $('.sidebar-wrapper').css('transition', 'all 0.5s ease');
                $('.bodyOverlay').show();
                $('html, body').css('overflow', 'hidden');
                flag = 1;
            } else {
                $('.sidebar-wrapper').css('margin-left', '-235px');
                $('.sidebar-wrapper').css('transition', 'all 0.5s ease');
                $('.bodyOverlay').hide();
                $('html, body').css('overflow', 'initial');
                flag = 0;
            }
        });

        $(".bodyOverlay").click(function() {
            $('.sidebar-wrapper').css('margin-left', '-235px');
            $('.sidebar-wrapper').css('transition', 'all 0.5s ease');
            $('.bodyOverlay').hide();
            $('html, body').css('overflow', 'initial');
            flag = 0;
        });


        // For mobile user name dropdown in sidebar
        mobileflag = 0;
        $(".mobileUserName").click(function() {
            if (mobileflag == 0) {
                $('.mobileProfileBox').css('height', '100%');
                $('.mobile-arrow-up').show();
                $('.mobile-arrow-down').hide();
                mobileflag = 1;
            } else {
                $('.mobileProfileBox').css('height', 'initial');
                $('.mobile-arrow-up').hide();
                $('.mobile-arrow-down').show();
                mobileflag = 0;
            }
        });

        // For searchbox tranistions in mobile

        $('.mobileSearchContainer').hide();

        $(".mobile-search-icon").click(function() {
            $('.mobileSearchContainer').show(500);
        });

        $(".crossIconMobile").click(function() {
            $('.mobileSearchContainer').hide(500);
        });


        //+++++++++++++++++ For Notification Box: +++++++++++++
        $('.notificationBox').hide();

        $("body").click(function(e) {
            if ($(e.target).is('.notificationBox')) {
                e.preventDefault();
            } else if ($(e.target).is('.top-notification-box') || $(e.target).is('.top-notification-box i')) {
                $('.notificationBox').show();
            } else {
                $('.notificationBox').hide();
            }
        });

        $('.notificationBox').click(function(event) {
            event.stopPropagation();
        });

        //+++++++++++++++++ For Notification Box: Ends +++++++++++++




    });

</script>



<script>
    $(document).ready(function() {

        //Initialize tooltips
        $('.nav-tabs > li > a[title~=Step]').tooltip();

        //Wizard
        $('a[data-toggle="tab"]').on('show.bs.tab', function(e) {

            var $target = $(e.target);

            if ($target.parent().hasClass('disabled')) {
                return false;
            }
        });

        $(".next-step").click(function(e) {

            var $active = $('.wizard .nav-tabs li.active');
            $active.next().removeClass('disabled');
            nextTab($active);


        });
        $(".prev-step").click(function(e) {

            var $active = $('.wizard .nav-tabs li.active');
            prevTab($active);

        });
    });

    function nextTab(elem) {
        $(elem).next().find('a[data-toggle="tab"]').click();
    }

    function prevTab(elem) {
        $(elem).prev().find('a[data-toggle="tab"]').click();
    }

</script>
</body>

