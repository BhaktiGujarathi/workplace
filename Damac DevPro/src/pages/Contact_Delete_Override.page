<apex:page id="myPage" standardController="Contact" extensions="Contact_Edit_Delete_Override_Controller" sidebar="false" showHeader="false">
    <script>
    if ( {!canDeleteRecord} ) {
        window.parent.location.href = "{!URLFOR($Action.Contact.Delete,Contact.id,[retURL=$Request.retURL],true)}";
    } else {
        alert("You are not allowed to delete the contact.");
        window.history.go(-1);
    }
    </script>
</apex:page>