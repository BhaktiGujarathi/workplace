<apex:page controller="promotionProcessController">
<html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" lang="en">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="x-ua-compatible" content="ie=edge" />
        <title>Tenant Status Matrix Grid</title>
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <!-- Import the Design System style sheet -->
        <!--<apex:stylesheet value="{!URLFOR($Resource.rssSLDS,'assets/styles/salesforce-lightning-design-system-vf.min.css')}" />-->
        <apex:slds />
    </head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <apex:form >
    <body>
    <div class="slds-scope">
    <table id="initiation" class="slds-table slds-table_bordered slds-table_cell-buffer slds-table_col-bordered slds-table_striped initiation">
        <thead>
            <tr class="slds-text-title_caps">
                <th scope="col" colspan="12">
                    <div class="slds-hyphenate" title="Verify the Tenant" align="center"><h2><b>Service Request - Request for Additional Parking</b></h2></div>
                    
                </th>
            </tr>
        </thead>
        <tr class="slds-text-title_caps">
            <td scope="col" colspan="3">
                <div class="slds-hyphenate" title="Verify the Tenant" align="left"><b>Stages</b></div>
            </td>
            <td scope="col" colspan="3" style="background-color:#6e97d8">
                <div class="slds-hyphenate" title="Verify the Tenant" align="left">1. Draft Requested</div>
            </td>
            <td scope="col" colspan="3">
                <div class="slds-hyphenate" title="Verify the Tenant" align="left">2. Request Submitted</div>
            </td>
            <td scope="col" colspan="3">
                <div class="slds-hyphenate" title="Verify the Tenant" align="left">3. Payment Received</div>
            </td>
        </tr>
        <tr class="slds-text-title_caps">
            <td scope="col" colspan="3">
                
            </td>
            <td scope="col" colspan="3">
                <div class="slds-hyphenate" title="Verify the Tenant" align="left">4. Document Verified</div>
            </td>
            <td scope="col" colspan="3">
                <div class="slds-hyphenate" title="Verify the Tenant" align="left">5. Confirmed</div>
            </td>
            <td scope="col" colspan="3">
                
            </td>
        </tr>
    </table>
    </div>
    </body>
    </apex:form>
    </html>
</apex:page>