<apex:page id="myPage" standardController="Booking__c" extensions="Booking_Edit_Delete_Override_Controller" sidebar="false" showHeader="false">
    <script>
    
    if ( {!canEditRecord} ) {
        window.parent.location.href = "{!URLFOR($Action.Booking__c.Edit,Booking__c.id,[retURL=URLFOR($Action.Booking__c.View,Booking__c.id)],true)}";
    } else {
        alert("You are not allowed to edit the Booking.");
        window.history.go(-1);
    }
    </script>
</apex:page>