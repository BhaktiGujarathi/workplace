    <fields>
        <fullName>STL_Operation_Month__c</fullName>
        <externalId>false</externalId>
        <formula>CASE((MONTH(DATEVALUE(CreatedDate)) + 1),
2, &quot;February&quot;,
3, &quot;March&quot;, 
4, &quot;April&quot;,
5, &quot;May&quot;,
6, &quot;June&quot;,
7, &quot;July&quot;,
8, &quot;August&quot;,
9, &quot;September&quot;,
10, &quot;October&quot;,
11, &quot;November&quot;,
12, &quot;December&quot;,
13, &quot;January&quot;,
&quot;&quot;)

+ &quot; &quot; +

IF(MONTH(DATEVALUE(CreatedDate) +1) &lt; 13, TEXT(YEAR(DATEVALUE(CreatedDate))), TEXT(YEAR(DATEVALUE(CreatedDate)) + 1))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
