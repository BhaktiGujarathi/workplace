    <validationRules>
        <fullName>Must_Fill_Out_App_Rejection_Reason</fullName>
        <active>true</active>
        <description>Must fill out rejection reason if you deny an application at the application level</description>
        <errorConditionFormula>AND( ISPICKVAL(Status__c, &quot;Denied&quot;), ISBLANK(STL_Rejection_Reason__c))</errorConditionFormula>
        <errorDisplayField>STL_Rejection_Reason__c</errorDisplayField>
        <errorMessage>Please fill out why you are rejecting this application.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Approve_After_All_Elements_Approved</fullName>
        <active>true</active>
        <description>Can only change an overall application status to approved once all discrete items on an application have been verified/validated.</description>
        <errorConditionFormula>AND(
OR(
 STL_Documentation__c = FALSE, 
 STL_Monthly_Average_Travel_Data__c = FALSE, 
 STL_No_Outstanding_Citations__c = FALSE,
STL_No_Outstanding_Invoices__c = FALSE,  
 STL_Routing__c = FALSE, 
 STL_Tech_Contractor__c = FALSE, 
 STL_Vehicle_Registration__c = FALSE),  ISPICKVAL(Status__c, &quot;Approved&quot;))</errorConditionFormula>
        <errorMessage>Please ensure all elements of this application are validated before approving the overall application.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Submit_if_all_9_documents_attached</fullName>
        <active>true</active>
        <description>There are 9 documents that need to be attached before being submitted</description>
        <errorConditionFormula>AND( STL_I_agree__c = True, Number_of_Documents__c &lt; 9)</errorConditionFormula>
        <errorMessage>Not all required documentation has been attached to this application.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STL_Operators_Can_Only_Select_Submitted</fullName>
        <active>true</active>
        <description>Prevents shuttle operators from changing application status to anything but &quot;In Process&quot; or &quot;Submitted - Pending Approval&quot;</description>
        <errorConditionFormula>AND(
$Profile.Name  = &quot;Customer Community Login 2&quot;,
ISCHANGED(Status__c),
NOT(OR(TEXT(Status__c) = &quot;In Process&quot;,TEXT(Status__c) = &quot;Submitted - Pending Approval&quot;)))</errorConditionFormula>
        <errorMessage>Please select &quot;Submitted - Pending Approval&quot; if you are ready to submit your application. Otherwise, leave it at &quot;In Process&quot; until you are ready to do so.</errorMessage>
    </validationRules>
</CustomObject>
