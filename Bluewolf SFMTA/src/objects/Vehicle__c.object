    <validationRules>
        <fullName>Registration_Expiration_Must_Be_Future</fullName>
        <active>false</active>
        <errorConditionFormula>Registration_Expiration_Date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Registration_Expiration_Date__c</errorDisplayField>
        <errorMessage>Your vehicle&apos;s registration cannot be expired.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rejection_Reason_Must_Be_Filled_Out</fullName>
        <active>true</active>
        <description>Must fill out rejection reason if you reject at the vehicle level</description>
        <errorConditionFormula>AND( ISPICKVAL(Status__c, &quot;Rejected&quot;),  ISBLANK(TEXT(STL_Rejection_Reason__c)))</errorConditionFormula>
        <errorDisplayField>STL_Rejection_Reason__c</errorDisplayField>
        <errorMessage>Please fill out why you are rejecting this vehicle.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>STL_Operators_Can_Only_Select_Submitted</fullName>
        <active>true</active>
        <description>Prevents shuttle operators from changing vehicle status to anything but &quot;Inactive&quot; or &quot;Submitted - Pending Approval&quot;</description>
        <errorConditionFormula>AND( 
$Profile.Name = &quot;Customer Community Login 2&quot;, 
ISCHANGED(Status__c), 
NOT(OR(TEXT(Status__c) = &quot;Inactive&quot;,TEXT(Status__c) = &quot;Submitted - Pending Approval&quot;)))</errorConditionFormula>
        <errorMessage>Please wait for SFMTA to approve or deny your vehicle submission. Select Submitted when you are ready to submit or Inactive to deactivate this vehicle from your fleet.</errorMessage>
    </validationRules>
</CustomObject>
