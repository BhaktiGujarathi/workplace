        <fullName>New_Vehicles</fullName>
        <booleanFilter>1 AND 2</booleanFilter>
        <columns>NAME</columns>
        <columns>Account__c</columns>
        <columns>License_Plate__c</columns>
        <columns>Vehicle_Make__c</columns>
        <columns>Vehicle_Model__c</columns>
        <columns>Vehicle_Year__c</columns>
        <columns>Status__c</columns>
        <columns>Registration_Expiration_Date__c</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CREATED_DATE</field>
            <operation>lessOrEqual</operation>
            <value>LAST_N_DAYS:7</value>
        </filters>
        <filters>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>In Progress,Submitted - Pending Approval</value>
        </filters>
        <label>New Vehicles</label>
        <sharedTo>
            <allInternalUsers></allInternalUsers>
        </sharedTo>
    </listViews>
