    <validationRules>
        <fullName>Cannot_add_line_items_to_PaidSent</fullName>
        <active>true</active>
        <description>Cannot add line items to invoice status paid</description>
        <errorConditionFormula>AND(
Hours__c &gt;0.01,
  ISNEW(),
OR(
Invoice_Status__c =&quot;Paid&quot;,
Invoice_Status__c =&quot;Sent&quot;),
Invoice__r.Allow_Add_l_Hours_When_Paid_Sent__c = false
)</errorConditionFormula>
        <errorMessage>Cannot add line items to a Paid or Sent Invoice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_add_line_items_to_pending_send</fullName>
        <active>true</active>
        <description>Cannot add line items to invoice status pending send</description>
        <errorConditionFormula>AND(
IF( 
Invoice_Status__c =&quot;Pending \&quot;Send\&quot;&quot;,true,false),
  ISNEW(),
 Invoice__r.Allow_Add_l_Hours_When_Paid_Sent__c = false
)</errorConditionFormula>
        <errorMessage>Cannot add line items to Invoice Status Pending Send</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contractor_hours_without_rate</fullName>
        <active>false</active>
        <description>Contractor hours without rate</description>
        <errorConditionFormula>AND(
 not(ISBLANK(Contractor__c )), 
 ISBLANK(Contractor_Rate__c  ) 
)</errorConditionFormula>
        <errorDisplayField>Contractor_Rate__c</errorDisplayField>
        <errorMessage>Please add Contractor Rate. You cannot add Contractor Hours without Contractor Rate</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contractor_rate_without_hours</fullName>
        <active>true</active>
        <description>Contractor_rate_without_hours</description>
        <errorConditionFormula>AND(
 ISBLANK(Contractor__c ), 
 NOT(ISBLANK(Contractor_Rate__c ) ) 
)</errorConditionFormula>
        <errorDisplayField>Contractor__c</errorDisplayField>
        <errorMessage>Please add the &quot;Contractor&quot; name. You cannot add &quot;Contractor Rate&quot; without &quot;Contractor&quot; name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LI_ACCTID_Not_matching_Inv</fullName>
        <active>true</active>
        <description>AccountID on Line Item not matching AccountID on Invoice</description>
        <errorConditionFormula>AND(
NOT( Invoice__r.Id = &apos;a096000000MdejF&apos;),
 Opportunity__r.AccountId &lt;&gt;   Invoice__r.Account_ID__c 
)</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>AccountID on Line Item not matching AccountID on Invoice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Line_Item_Cannot_Be_Modified_When_Sent</fullName>
        <active>false</active>
        <description>AND(
ISCHANGED( LastModifiedDate ),
OR(
NOT(ISBLANK( Invoice__r.Date_Sent__c )),
 CONTAINS(TEXT(Invoice__r.Status__c ), &quot;Sent&quot;) 
)
)</description>
        <errorConditionFormula>AND(
ISCHANGED( Hours__c  ),
NOT(ISBLANK( Invoice__r.Date_Sent__c ))
)</errorConditionFormula>
        <errorMessage>Line Items Cannot Be Modified Once the Invoice has been Sent to the Client (Invoice Date Sent &lt;&gt; Null)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_Credit_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Credit_Hrs__c &gt; Hours_Num__c ,
 ISBLANK(TEXT( Credit_Reason__c )) 
)</errorConditionFormula>
        <errorDisplayField>Credit_Reason__c</errorDisplayField>
        <errorMessage>Please add Credit Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NotToExeedHours</fullName>
        <active>false</active>
        <errorConditionFormula>Hours__c &gt;NotToExceed__c</errorConditionFormula>
        <errorDisplayField>NotToExceed__c</errorDisplayField>
        <errorMessage>NotToExeedHours - Please check the time log</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_to_be_less_than_1</fullName>
        <active>false</active>
        <errorConditionFormula>(Hours__c + Credit_Hrs__c)  &lt;0.001</errorConditionFormula>
        <errorMessage>No Hours Have Been Added</errorMessage>
    </validationRules>
