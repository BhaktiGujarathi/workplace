    <validationRules>
        <fullName>Approved_Ready_Only_by_Henry</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Status__c, &quot;Open - Approved &amp; Ready to Send&quot;),
 NOT( $User.Id   = &apos;00560000001vAOX&apos;)
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Only Henry Abenaim can approve Invoices. Please change the status to Open - Pending HA Review&quot; and that will send a notification to Henry to review the invoice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Cancle_with_open_Balance</fullName>
        <active>true</active>
        <description>Cannot cancel the invoice if there is an open balance. Need to first add a credit line item.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Status__c, &quot;Canceled&quot;) ,
 Total_Amount_Due__c &gt;1
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Cannot mark the Invoice Status as &quot;Canceled&quot; when there is an open balance. Need to first add a credit line item to reduce the Amount Due.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_mark_as_Paid_as_there_is_no_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
  ISBLANK(Date_Sent__c) , 
ISPICKVAL(Status__c, &quot;Paid&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Date_Sent__c</errorDisplayField>
        <errorMessage>Cannot mark as Paid as there is no Date Sent. Please update the Date Sent</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_send_without_Approval</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
 Approved_to_Send__c =false, 
ISPICKVAL(Status__c,&quot;Sent&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Approved_to_Send__c</errorDisplayField>
        <errorMessage>Cannot Send Invoices when Approved_to_Send__c = false. That checkbox needs to be true.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Confirm_contractor_hours_prior_to_Send</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
Updated_Contractor_Hours__c  = false,
ispickval(Status__c, &quot;Sent&quot;)
)</errorConditionFormula>
        <errorDisplayField>Updated_Contractor_Hours__c</errorDisplayField>
        <errorMessage>Need to Update Contractor Hours prior to sending the invoice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Paid_and_Status_Not_Paid</fullName>
        <active>true</active>
        <description>If Status=Paid then need date paid. If date paid is populated, then need Status=Paid</description>
        <errorConditionFormula>OR(
AND( 
ISBLANK(Date_Paid__c) ,
ISPICKVAL(Status__c, &quot;Paid&quot;) 
),
AND(
NOT(ISBLANK(Date_Paid__c)),
NOT(CONTAINS(TEXT(Status__c), &quot;Paid&quot;))
)
)</errorConditionFormula>
        <errorDisplayField>Date_Paid__c</errorDisplayField>
        <errorMessage>If Status=Paid then need date paid. If date paid is populated, then need Status=Paid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Sent_blank_wen_Status_Sent</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK( Date_Sent__c ) ,
 ISPICKVAL(Status__c,&quot;Sent&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Date_Sent__c</errorDisplayField>
        <errorMessage>If Status=Sent then need Date Sent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_to_mark_Approved_to_Send_Checkbox</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Status__c,&quot;Open - Approved &amp; Ready to Send&quot;),
 Approved_to_Send__c= false)</errorConditionFormula>
        <errorDisplayField>Approved_to_Send__c</errorDisplayField>
        <errorMessage>Need to mark Approved to Send Checkbox in order to change status to Open - Approved &amp; Ready to Send</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Henry_Can_change_Approved_to_Send</fullName>
        <active>true</active>
