        <fullName>Bill_as_Paid_when_there_is_not_Amount_Pa</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
AND( 
ISBLANK(Date_Paid__c) , 
ISPICKVAL(Status__c, &quot;Paid&quot;) 
),
AND(
Bill_Amount_Paid__c &lt;0.05,
ISPICKVAL( Status__c, &quot;Paid&quot;) 
)
)</errorConditionFormula>
        <errorMessage>Cannot mark the Bill as Paid when there is not Amount Paid or Date Paid</errorMessage>
    </validationRules>
