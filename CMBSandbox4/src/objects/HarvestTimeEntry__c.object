    <fields>
        <fullName>Date_this_Week__c</fullName>
        <externalId>false</externalId>
        <formula>if(
AND(
 Date_in_of_Weeks__c =
IF( 
  CEILING( ( Today() - DATE( YEAR( Today() ), 1, 1) + 1) / 7) &gt; 52,
  52,
  CEILING( ( Today() - DATE( YEAR( Today() ), 1, 1) + 1) / 7)),

YEAR( Date__c )=
YEAR(Today())) ,
true ,false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Is the date this week?</inlineHelpText>
