    <fields>
        <fullName>Hours_For_Client__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
 CONTAINS( Task_Type__c , &quot;Non-Billable to Client&quot;),0,
IF(
 CONTAINS( Task_Type__c , &quot;Shared Client Call&quot;),0,
IF(
 CONTAINS(  HarvestProject__r.Account__r.Name , &quot;CloudMyBiz&quot;),null,

IF(ISBLANK(Developer_Company__r.Contractor_Hour_Ratio__c), (Duration__c + IF(Credit_Hrs__c &gt;0.01, - Credit_Hrs__c, 0)),


IF(Credit_Hrs__c&gt;0,
(Duration__c - Credit_Hrs__c),

(Duration__c * Developer_Company__r.Contractor_Hour_Ratio__c))))))</formula>
        <label>Hours for Client</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
