    <webLinks>
        <fullName>Apply_Change_Order</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Apply Change Order</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}
try{
  var today = new Date(); 
  var dd = today.getDate(); 
  var mm = today.getMonth()+1; //January is 0! 
  var yyyy = today.getFullYear(); 

  if(dd&lt;10) { 
   dd=&apos;0&apos;+dd 
  } 

  if(mm&lt;10) { 
    mm=&apos;0&apos;+mm 
  } 
  today = yyyy+&apos;-&apos;+mm+&apos;-&apos;+dd; 
  var objEstimate= new sforce.SObject(&quot;Estimates__c&quot;);
  objEstimate.Name = &quot;{!Opportunity.Name}-Estimates&quot;;
  objEstimate.Harvest_Projects__c = &quot;{!Opportunity.ProjectId__c}&quot;;
  objEstimate.Opportunity__c = &quot;{!Opportunity.Id}&quot;;
objEstimate.Estimates_Timeframe__c = &quot;{!Opportunity.Anticipated_Duration__c}&quot;;
objEstimate.Estimates_Hours__c = {!Opportunity.Total_SOW_Hours__c};
  objEstimate.Date_Sent__c =today;
  objEstimate.Status__c = &quot;Open - Not Sent Yet&quot;;
 
  var createResult = sforce.connection.create([objEstimate]);  
  if (createResult[0].getBoolean(&quot;success&quot;)) {     
    window.location.href=&quot;/&quot;+createResult[0].id+&quot;/e?retURL=&quot;+createResult[0].id; 
  }else{ 
    var message = &apos;Failed To Create New Invoice Record.&apos;; 
    if(createResult[0].errors.message != null) 
      message += &apos;\n\nError Message:\n&apos;+createResult[0].errors.message; 
    alert(message); 
} 
}catch (exc) { 
  alert(&apos;Exception : &apos;+exc.message); 
}</url>
    </webLinks>
