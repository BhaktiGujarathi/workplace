{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 

var month = new sforce.SObject(&quot;Accounting_Calendar_Month__c&quot;);
month.Id = &apos;{!Accounting_Calendar_Month__c.Id}&apos;;
month.Locked__c = true;
if (confirm(&quot;Are you sure you want to lock this month?&quot;)) {

    var result = sforce.connection.update([month]);

	if(result[0].getBoolean(&quot;success&quot;)) {
   	
		window.location.reload();
	}
	else{
  	
  		alert(&apos;Error : &apos; + result);
	}
}</url>
