    <validationRules>
        <fullName>Account_Complete_Validation</fullName>
        <active>false</active>
        <description>Verifies that the Account_Complete field on the Account is True when the Opportunity Stage is changed to Application In, Underwriting, Approved, Agreement Requested, Agreement Sent, Agreement Signed or Funded if the Opportunity Type Category is New.</description>
        <errorConditionFormula>IF(AND(Type_Category__c=&quot;New&quot;, OR (ISPICKVAL(StageName, &quot;Application In&quot;), ISPICKVAL(StageName, &quot;Underwriting&quot;), ISPICKVAL(StageName, &quot;Approved&quot;), ISPICKVAL(StageName, &quot;Agreement Requested&quot;), ISPICKVAL(StageName, &quot;Agreement Sent&quot;), ISPICKVAL(StageName, &quot;Agreement Signed&quot;), ISPICKVAL(StageName, &quot;Funded&quot;)), Account.Account_Complete__c = FALSE),True,False)</errorConditionFormula>
        <errorMessage>The information on the Account in incomplete.  Please complete the Account Record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Approve_Sub_Sales_Insufficent</fullName>
        <active>false</active>
        <description>Cannot approve submission without more than 10K in Gross monthly sales.</description>
        <errorConditionFormula>AND ( McaApp__Gross_monthly_Sales__c &lt; 10000,  DATEVALUE (Submission_Approved_DTS__c ) &gt;  DATE(2016,06,28),  Submission_Approved__c = TRUE)</errorConditionFormula>
        <errorMessage>You cannot approve this submission if the &quot;Gross Monthly Sales&quot; is less than $10,000.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Approve_Subs_Missing_info</fullName>
        <active>false</active>
        <description>Prevent submission approval if stage is Application missing info.</description>
        <errorConditionFormula>AND( ISPICKVAL( StageName , &quot;Application Missing Info&quot;), Submission_Approved__c = TRUE,  DATEVALUE (Submission_Approved_DTS__c) &gt;  DATE(2016,06,28))</errorConditionFormula>
        <errorMessage>You cannot approve this submission if you are setting the Stage to &quot; Application Missing Info &quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dupe_Storage</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISPICKVAL(StageName,&apos;Closed Lost - Duplicate&apos;), OR( OwnerId != &apos;005o0000002OiBg&apos;, OwnerId != &apos;005o00000034KIr&apos;, OwnerId != &apos;005o00000034KIN&apos; ))</errorConditionFormula>
        <errorMessage>Duplicate Opportunities must be owned by storage account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExecutiveApprovalRequiredBeforeAcc</fullName>
        <active>false</active>
        <description>Forces the Executive to approve the Opportunity before Accounting.</description>
        <errorConditionFormula>Executive_Approval__c == FALSE
&amp;&amp;
 Approved_for_Great_Plains__c  == TRUE</errorConditionFormula>
        <errorMessage>You cannot approve this Opportunity until the Executive has approved it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ManagerApprovalRequiredBeforeExec</fullName>
        <active>false</active>
        <description>Forces the Manager to approve the Opportunity before the Executive.</description>
        <errorConditionFormula>Manager_Approval__c == FALSE
&amp;&amp;
 Executive_Approval__c == TRUE</errorConditionFormula>
        <errorMessage>You cannot approve this Opportunity until the Manager has approved it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manager_Approve</fullName>
        <active>false</active>
        <description>Accounting cannot approve an opportunity for Great Plains unless it has been Manager, Executive, and Rep. approved.</description>
        <errorConditionFormula>Manager_Approval__c =FALSE
&amp;&amp;
 Executive_Approval__c = TRUE</errorConditionFormula>
        <errorDisplayField>Amt_Monthly_Bank_Deposits__c</errorDisplayField>
        <errorMessage>This record has not been Manager Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>McaApp__Required_Reason_Lost_for_Opportunity</fullName>
        <active>false</active>
        <description>When an Opportunity is marked as &apos;Dead&apos; or &apos;Lost&apos;, the user should provide a value in &apos;Reason Lost&apos;</description>
        <errorConditionFormula>ISPICKVAL( StageName , &apos;Closed Lost&apos;) 
&amp;&amp; 
ISBLANK( TEXT(McaApp__Reason_Lost__c))</errorConditionFormula>
        <errorDisplayField>McaApp__Reason_Lost__c</errorDisplayField>
        <errorMessage>Please provide a reason for Opportunity being marked as &apos;Closed Lost&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opener_and_their_Stages</fullName>
        <active>false</active>
        <description>Prevents the Opener from choosing a stage past Application In.</description>
        <errorConditionFormula>AND( $UserRole.Name = &quot;Opener&quot;, NOT(OR(ISPICKVAL(StageName, &quot;Application Sent&quot;), ISPICKVAL(StageName, &quot;Application In&quot;), ISPICKVAL(StageName, &quot;Closed Lost - Merchant&quot;), ISPICKVAL(StageName, &quot;Application Missing Info&quot;), ISPICKVAL(StageName, &quot;Agreement Requested&quot;), ISPICKVAL(StageName, &quot;Agreement Sent&quot;), ISPICKVAL(StageName, &quot;Agreement Signed&quot; ))))</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>You cannot set the stage of the Opportunity to beyond Application In.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opp_Amount_Requested_Null</fullName>
        <active>false</active>
        <description>If the Opp Complete check box is not marked true and the Account Complete check box is, the error will appear</description>
        <errorConditionFormula>IF(AND(Test_Complete__c == false, Type_Category__c=&quot;New&quot;, OR (ISPICKVAL(StageName, &quot;Application In&quot;), ISPICKVAL(StageName, &quot;Underwriting&quot;), ISPICKVAL(StageName, &quot;Approved&quot;), ISPICKVAL(StageName, &quot;Agreement Requested&quot;), ISPICKVAL(StageName, &quot;Agreement Sent&quot;), ISPICKVAL(StageName, &quot;Agreement Signed&quot;), ISPICKVAL(StageName, &quot;Funded&quot;)),ISNULL ( McaApp__Amount_Requested__c ) , Account.Account_Complete__c =True), true, false)</errorConditionFormula>
        <errorDisplayField>McaApp__Amount_Requested__c</errorDisplayField>
        <errorMessage>Please fill in amount requested.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opp_Complete_False_Account_Complete_True</fullName>
        <active>false</active>
        <description>If the Opp Complete check box is not marked true and the Account Complete check box is, the error will appear</description>
        <errorConditionFormula>IF(AND(Type_Category__c=&quot;New&quot;, OR (ISPICKVAL(StageName, &quot;Application In&quot;), ISPICKVAL(StageName, &quot;Underwriting&quot;), ISPICKVAL(StageName, &quot;Approved&quot;), ISPICKVAL(StageName, &quot;Agreement Requested&quot;), ISPICKVAL(StageName, &quot;Agreement Sent&quot;), ISPICKVAL(StageName, &quot;Agreement Signed&quot;), ISPICKVAL(StageName, &quot;Funded&quot;)),Opportunity_Complete__c =False , Account.Account_Complete__c =True), true, false)</errorConditionFormula>
        <errorMessage>Opportunity is Incomplete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opp_Complete_False_Acct_Complete_False</fullName>
