                                            $tgt.removeClass('placeHolder');
                                        }
                                    });
                                    $('.placeHolder-'+index).on( "blur",  function(event){
                                        $tgt = $(event.target);
                                        if($tgt.val() === '' ){
                                            $('[id$=searchAccountId]').val('');
                                            $('[id$=searchPos]').val(index);
                                            CallApexMethod();
                                        }
